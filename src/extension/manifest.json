{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "2.0.8",
  "manifest_version": 2,
  "offline_enabled" : true,
  "default_locale": "en",
  "kiosk_enabled": true,
  "oauth2": {
    "client_id": "1066229426336-mq4dh1q2ufe2d8hp0un5jfjft8n0oc8o.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/cloudprint"
    ]
  },
  "requirements": {
    "3D": {
      "features": ["webgl"]
    }
  },
  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "videoCapture",
    { "fileSystem": ["write"] },
    "contextMenus",
    "storage",
    "power",
    "identity",
    "https://www.google.com/cloudprint/search",
    "https://www.google.com/cloudprint/submit",
    "http://camera-kiosk.appspot.com/send"
  ],
  "sandbox": {
    "pages": [ "app/index.html" ],
    "content_security_policy": "sandbox allow-scripts; default-src 'self'; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval' 'unsafe-inline'; img-src 'self' data:"
  }
}
